{
    "version": 2,
    "name": "carbon-tracker-backend",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/favicon.ico",
            "status": 204
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/(.*)",
            "dest": "/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}